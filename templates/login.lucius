#main{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
}
div.loginbox{
  display:block;
  h3{
   text-align: center;
  }
  border: black;
  background-color: blanchedalmond;
  border-width: 1px;
  border-style: outset;
}


a.facebookLogin {
    padding: 10px 48px;
    background: #3C5A99;
    color: #FFF;
    text-decoration: none;
    background-size: 48px 43px;
    background-position-x: 0px;
    background-repeat: no-repeat;
    display: inline-block;
    width: 250px;
    text-align: center;
    background-image:url(data:image/svg+xml#{semicolon}base64,);
}

a.googleLogin {
    padding: 10px 48px;
    background: #dd4b39;
    color: #FFF;
    text-decoration: none;
    background-size: 52px 53px;
    display: inline-block;
    width: 250px;
    text-align: center;
    background-repeat: no-repeat;
    background-image:url(data:image/png#{semicolon}base64,);
}

div.personaLogin > p > a {
  height: 50px;
  width: 250px;
  display: inline-block;
  background:url(data:image/png#{semicolon}base64,) 0 0 no-repeat;
  background-color: gray;
  img {
    display: none;
  }
}