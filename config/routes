-- youtube
/api/oauth2/youtube/channels          YTChannelsR   -- user channels
/api/oauth2/youtube/playlists         YTPlaylistsBaseR
/api/oauth2/youtube/playlists/#String YTPlaylistsR  -- playlists for a channel
/api/oauth2/youtube/playlist          YTPlaylistItemBaseR
/api/oauth2/youtube/playlist/#String  YTPlaylistItemR
/api/oauth2/youtube/allvideos         YTAllVideosR  -- all videos for user

-- /oauth2/youtube/video             YTVideoBaseR
/api/oauth2/youtube/video/*Texts      YTVideoR      GET  -- video details by id

-- normal authentication level
/api/user/channels                    UserChannelsR GET


