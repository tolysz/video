function($scope,$http,$log){

    $scope.getList = function(){
        $http.get("@{AllUserR}")
            .success( function(data){
                $log.debug(data);
                $scope.users = _.map(data, function(item){
                 item.someName = item.userFriendly || item.userName || item.userIdent;
                 return item;
                })
            }
        )
    }

    $scope.getList()
}
