.layout-content  [layout] > div {
  padding: 8px;
  box-shadow: 0px 2px 5px 0 rgba(0,0,0,0.26);
  opacity: 0.9;
  color: white;
}
.layout-content [layout] > div:nth-child(1) {
  background-color: #009688;
}
.layout-content [layout] > div:nth-child(2) {
  background-color: #3949ab;
}
.layout-content [layout] > div:nth-child(3) {
  background-color: #9c27b0;
}
.layout-content md-divider {
  margin-top: 16px;
}
